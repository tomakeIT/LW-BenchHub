{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "train_rl",
            "type": "python",
            "request": "launch",
            "args": [
                "--task_config=lerobot_liftobj_state",
                "--headless",
            ],
            "program": "${workspaceFolder}/policy/maniskill_ppo/train.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "camera_alignment",
            "type": "python",
            "request": "launch",
            "args": [
                "--task_config=lerobot_liftobj_state",
                // "--headless",
            ],
            "program": "${workspaceFolder}/policy/skrl/camera_alignment.py",
            "console": "integratedTerminal",
            "justMyCode": false, 
        },
        {
            "name": "rl runner",
            "type": "python",
            "request": "launch",
            "args": [
            ],
            "program": "${workspaceFolder}/samples/distributed_rl/runner.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "env_server",
            "type": "python",
