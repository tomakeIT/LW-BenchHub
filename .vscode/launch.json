{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "train_rl",
            "type": "python",
            "request": "launch",
            "args": [
                "--task_config=lerobot_liftobj_state",
                "--headless",
            ],
            "program": "${workspaceFolder}/policy/maniskill_ppo/train.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "eval_rl",
            "type": "python",
            "request": "launch",
            "args": [
                "--task_config=lerobot_liftobj_state_play",
            ],
            "program": "${workspaceFolder}/lw_benchhub/scripts/rl/play.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "camera_alignment",
            "type": "python",
            "request": "launch",
            "args": [
                "--task_config=lerobot_liftobj_state",
                // "--headless",
            ],
            "program": "${workspaceFolder}/policy/skrl/camera_alignment.py",
            "console": "integratedTerminal",
            "justMyCode": false, 
        },
        {
            "name": "rl runner",
            "type": "python",
            "request": "launch",
            "args": [
            ],
            "program": "${workspaceFolder}/samples/distributed_rl/runner.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "env_server",
            "type": "python",
            "request": "launch",
            "args": [
                "--remote_protocol=ipc",
            ],
            "program": "${workspaceFolder}/lw_benchhub/scripts/env_server.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "LD_PRELOAD": "/usr/lib/x86_64-linux-gnu/libjemalloc.so.2",
            },
        },
        {
            "name": "eval_policy",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "--config=policy/GR00T/deploy_policy_piper.yml",
                // "--overrides",
                // "--env_cfg:task", "SizeSorting",
                // "--env_cfg:layout", "robocasakitchen",
                // "--instruction", "Stack objects on counter from large to small", 
                // "--time_out_limit", "60",
                // "--test_num", "5"
            ],
            "program": "${workspaceFolder}/lw_benchhub/scripts/policy/eval_policy.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "teleop",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "--task_config=pandaomron",
            ],
            "program": "${workspaceFolder}/lw_benchhub/scripts/teleop/teleop_main.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "replay",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "--num_envs=1",
                "--dataset_file=datasets/dataset.hdf5",
                "--width=480",
                "--height=480",
                "--enable_cameras",
                "--device=cpu"
            ],
            "program": "${workspaceFolder}/lw_benchhub/scripts/teleop/replay_demos.py",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "replay_action_action",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "--num_envs=1",
                "--dataset_file=datasets/dataset.hdf5",
